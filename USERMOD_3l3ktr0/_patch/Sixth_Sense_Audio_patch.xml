<patchs>
<!-- add the sound bank to audio_mods.xml -->
<patch>
  <type>xml</type>
  <mode>add</mode>
  <file>\\res_mods\\versiondir\\audioww\\audio_mods.xml</file>
  <path>//audio_mods.xml/loadBanks</path>
  <search></search>
  <replace>bank/sixthsense_suprise.bnk</replace>
</patch>
<!-- change the sixth sense sound path in sixthSense.xml -->
<patch>
  <type>xml</type>
  <mode>edit</mode>
  <file>\\res_mods\\sixthSense.xml</file>
  <path>//sixthSense.xml/soundPath</path>
  <search>.*</search>
  <replace>lightbulb_suprise</replace>
</patch>
<!-- First patch adds the event name -->
<patch>
  <type>xml</type>
  <mode>add</mode>
  <file>\\res_mods\\versiondir\\audioww\\audio_mods.xml</file>
  <path>//audio_mods.xml/events</path>
  <search></search>
  <replace>event/name/lightbulb</replace>
</patch>
<!-- second patch adds the event mod-->
<patch>
  <type>xml</type>
  <mode>add</mode>
  <file>\\res_mods\\versiondir\\audioww\\audio_mods.xml</file>
  <path>/audio_mods.xml/events/event[name = &quot;lightbulb&quot;]</path>
  <search></search>
  <replace>mod/lightbulb_suprise</replace>
</patch>
</patchs>